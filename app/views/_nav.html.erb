<header>
  <div class="hashtag"><a href="#">#ChiSpaceOdyssey</a></div>
  <% if logged_in? %>
    <div class="sign-in-out"><%= link_to 'Sign Out', signout_path %></div>
  <% else %>
    <div class="sign-in-out">
      <%= link_to '/auth/twitter' do %>
        <%= image_tag 'sign-in-with-twitter-gray.png', id: 'twitter' %>
      <% end %>
    </div>
  <% end %>
  <div id='logo'>
    <%= link_to image_tag("logo.png"), root_path %>
  </div>
  <div class="nav"><%= link_to "My Locations", show_path %>
    <%= link_to "Map", map_path  %>
  </div>
</header>